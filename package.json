    "build:debug": "cross-env ELEVENTY_ENV=prod DEBUG=Eleventy* eleventy",
    "build:netlify": "eleventy --config=netlify.eleventy.js"
  },
  "author": "James Steinbach",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.3.5",
    "axios": "^0.22.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.17.1",
    "cross-env": "^7.0.2",
    "cssnano": "^5.0.8",
    "eleventy-plugin-dropcap": "^1.0.1",
    "eleventy-plugin-toc": "^1.1.3",
    "eslint": "^6.0.0",
    "luxon": "^1.2.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^5.0.1",
    "markdown-it-highlightjs": "^3.5.0",
    "postcss": "^8.3.7",
    "sass": "^1.26.3",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "typogr": "^0.6.8"
  },
  "standard": {
    "globals": [
      "FormData",
      "addEventListener",
      "fetch",
      "localStorage"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "mailgun.js": "^5.0.1"
  }
}
